pkgname="sloc"
pkgver="0.1.0"
pkgrel="1"
pkgdesc="Count the number of lines of code in a directory"
arch=("any")
license=("GPLv3")
source=("sloc.sh")
sha256sums=('798b30f065622ca11fa7aa60ccf64b1bab2f0974994f5d93a73815c8d3ffdd7d')
url="https://github.com/AmateurECE/pacman"
groups=("edtwardy")

package() {
    install -d $pkgdir/usr/bin
    install -m755 sloc.sh $pkgdir/usr/bin/sloc
}
