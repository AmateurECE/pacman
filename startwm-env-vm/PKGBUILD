pkgname=startwm-env-vm
pkgver=1.0.0
pkgrel=1
pkgdesc='setup environment for startwm, to run Wayland in a virtual machine'
arch=(any)
url='https://github.com/AmateurECE/pacman.git'
license=('MIT')
source=('startwm.sh')
sha256sums=('47a45358fd31977d1bedec8460669396dcc6aa746d7aa509acd15a8fac623a83')

package() {
    install -Dm644 startwm.sh "${pkgdir}/etc/profile.d/startwm.sh"
}
