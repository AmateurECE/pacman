pkgname="sloc"
pkgver="0.1.0"
pkgrel="1"
pkgdesc="Count the number of lines of code in a directory"
arch=("any")
license=("GPLv3")
source=("sloc.sh")
sha256sums=('1cd8a2e09c9b6eb8d33514d52e2a7fdf82bc84f4fc3cd6f047a39b17226e2944')
url="https://github.com/AmateurECE/pacman"
groups=("edtwardy")

package() {
    install -d $pkgdir/usr/bin
    install -m755 sloc.sh $pkgdir/usr/bin/sloc
}
